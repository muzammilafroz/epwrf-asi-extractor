  "navigation_flow": [
    {
      "step": 1,
      "page": "index.aspx",
      "description": "Homepage",
      "action": "Click 'Annual Survey of Industries' link",
      "selector": "//a[contains(@href, 'TypesOfASIStateWise')]"
    },
    {
      "step": 2,
      "page": "TypesOfASIStateWise.aspx",
      "description": "ASI Type Selection",
      "action": "Select '2-Digit Industry State-wise' radio and click Proceed",
      "radio_id": "rad_grid_7",
      "proceed_button": "//input[@type='submit' and contains(@value, 'Proceed')]"
    },
    {
      "step": 3,
      "page": "ASITreeview_TwoDigitIndustry_State.aspx",
      "description": "Filter Page - Select all criteria",
      "actions": [
        {
          "order": 1,
          "name": "Select Series",
          "element": "ctl00_ContentPlaceHolder1_drpSeries",
          "method": "Select dropdown by visible text",
          "value": "EPWRF Concorded Series"
        },
        {
          "order": 2,
          "name": "Select NIC Code",
          "element": "chk_True_EPWRF Concorded Series_{nodeId}_{description}",
          "method": "Click checkbox",
          "example": "chk_True_EPWRF Concorded Series_168_manufacture of wearing apparel"
        },
        {
          "order": 3,
          "name": "Select States",
          "method": "Set hidden field OR use JavaScript",
          "hidden_field": "ctl00_ContentPlaceHolder1_hidList",
          "format": "State1|State2|State3|",
          "js_alternative": "drpstate._dropDownList.selectItemByIndex(i)"
        },
        {
          "order": 4,
          "name": "Select Variables",
          "method": "Use JavaScript to select in obout dropdown",
          "js": "cbkSubVariables._dropDownList.selectItemByIndex(i)",
          "hidden_field": "ctl00_ContentPlaceHolder1_hdSubVariables"
        },
        {
          "order": 5,
          "name": "Select Year Range",
          "start_element": "ctl00_ContentPlaceHolder1_drpStartYear",
          "end_element": "ctl00_ContentPlaceHolder1_drpEndYear",
          "method": "Select dropdown by value"
        },
        {
          "order": 6,
          "name": "Submit Form",
          "pre_action": "document.getElementById('ctl00_ContentPlaceHolder1_hdnsec1').value = ob_t2_list_checked();",
          "element": "ctl00_ContentPlaceHolder1_btnsubmit",
          "method": "Click"
        }
      ]
    },
    {
      "step": 4,
      "page": "ASI_TwoDigitDisplayData_Statewise.aspx",
      "description": "Data Display Page",
      "action": "Click Export to Excel button",
      "excel_button_id": "ctl00_ContentPlaceHolder1_ibtnexcel",
      "excel_button_type": "image",
      "excel_button_class": "btn_report_export_excel",
      "back_button_id": "Button2"
    }
  ],
