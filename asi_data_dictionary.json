  "storage_schema": {
    "description": "Recommended database schema for efficient storage",
    "fact_table": {
      "name": "asi_data",
      "columns": [
        {"name": "subsection_id", "type": "TINYINT", "bytes": 1},
        {"name": "series_id", "type": "TINYINT", "bytes": 1},
        {"name": "state_id", "type": "TINYINT", "bytes": 1, "nullable": true},
        {"name": "industry_id", "type": "SMALLINT", "bytes": 2},
        {"name": "variable_id", "type": "TINYINT", "bytes": 1},
        {"name": "year", "type": "SMALLINT", "bytes": 2},
        {"name": "value", "type": "FLOAT", "bytes": 4}
      ],
      "bytes_per_row": 12,
      "indexes": ["(subsection_id, series_id, state_id, industry_id, variable_id, year)"]
    },
    "dimension_tables": [
      "dim_subsections",
      "dim_series",
      "dim_states",
      "dim_industries_2digit",
      "dim_industries_3digit",
      "dim_industries_4digit",
      "dim_variables"
    ],
    "estimated_rows": {
      "2digit_state": "5 series × 60 industries × 39 states × 38 variables × 45 years = ~200M rows max",
      "3digit": "2 series × 170 industries × 38 variables × 30 years = ~400K rows",
      "4digit": "3 series × 200 industries × 38 variables × 20 years = ~450K rows",
      "total_estimated": "~5-10M actual data points (sparse matrix)"
    }
  },

